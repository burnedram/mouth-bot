{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2021", "esnext.asynciterable"],
    "module": "commonjs",
    "rootDir": "src",
    "emitDecoratorMetadata": true, // required for discordx
    "experimentalDecorators": true, // required for discordx
    "resolveJsonModule": true,
    "allowJs": false,
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "exclude": ["node_modules"]
}
